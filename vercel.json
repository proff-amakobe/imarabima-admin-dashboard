{
  "version": 2,
  "name": "imarabima-admin-dashboard",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://imarabima-platform-fitpf2zz6-proff-amakobes-projects.vercel.app"
  }
}
